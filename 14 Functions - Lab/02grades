def grades(result):
   if 2.00 <= points <= 2.99:
       return "Fail"
   elif 3.00 <= points <= 3.49:
       return "Poor"
   elif 3.50 <= points <= 4.49:
       return "Good"
   elif 4.50 <= points <= 5.49:
       return "Very Good"
   elif 5.50 <= points <= 6.00:
       return "Excellent"
   return

points=float(input())

print(grades(points))
